{
  "functions": {
    "app/api/reports/[id]/export-pdf/route.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 60,
      "memory": 2048
    },
    "app/api/allergy-cards/[id]/export-pdf/route.ts": {
      "runtime": "nodejs20.x", 
      "maxDuration": 60,
      "memory": 2048
    },
    "app/api/reports/[id]/print-view/route.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 30,
      "memory": 1024
    },
    "app/api/test-pdf/route.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 60,
      "memory": 2048
    },
    "app/api/reports/[id]/export-pdf-fallback/route.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 10,
      "memory": 512
    }
  },
  "env": {
    "VERCEL": "1",
    "AWS_EXECUTION_ENV": "AWS_Lambda_nodejs20.x",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "1"
  },
  "outputDirectory": ".next",
  "buildCommand": "npm run build"
}
